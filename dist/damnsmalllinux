#!/usr/bin/env bash

INFO="Damn Small Linux|-|https://www.damnsmalllinux.org/|The New DSL 2024 has been reborn as a compact Linux distribution tailored for low-spec x86 computers."

function dist_releases() {
    echo 2024
}

function dist_image_get() {
    local HASH=""
    local URL="https://www.damnsmalllinux.org/download"
    local ISO="dsl-${RELEASE}.rc3.iso"
    HASH="$(wget -q -O- "${URL}/${ISO}.md5.txt" | cut -d' ' -f1)"
    echo "${URL}/${ISO} ${HASH}"
}

