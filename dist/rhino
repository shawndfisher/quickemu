#!/usr/bin/env bash

INFO="Rhino Linux|-|https://rhinolinux.org/index.html|Rhino Linux is a rolling release Ubuntu-based distribution with Pacstall & XFCE at its core."

function dist_releases() {
    echo 2024.1
}

function dist_image_get() {
    local HASH=""
    local URL="https://sourceforge.net/projects/rhino-linux-builder/files/${RELEASE}"
    local ISO="Rhino-Linux-${RELEASE}-amd64.iso"
    HASH="$(web_pipe "${URL}/${ISO}.sha256" | cut -d" " -f1)"
    echo "${URL}/${ISO} ${HASH}"
}

