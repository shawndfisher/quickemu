#!/usr/bin/env bash

PRETTY_NAME="Rhino Linux"
HOMEPAGE="https://rhinolinux.org/index.html"
INFO="Rhino Linux|Debian|-|https://rhinolinux.org/index.html|Rhino Linux is a rolling release Ubuntu-based distribution with Pacstall & XFCE at its core."

function dist_releases() {
    echo 2024.1
}

function dist_image() {
    local HASH=""
    local ISO="Rhino-Linux-${RELEASE}-amd64.iso"
    local URL="https://sourceforge.net/projects/rhino-linux-builder/files/${RELEASE}"
    HASH="$(web_pipe "${URL}/${ISO}.sha256" | cut_1)"
    echo "${URL}/${ISO} ${HASH}"
}

