#!/usr/bin/env bash

INFO="Archcraft|-|https://archcraft.io/|Yet another minimal Linux distribution, based on Arch Linux."

function dist_releases() {
    echo latest
}

function dist_image_get() {
    local HASH=""
    local URL=""
    URL="https://sourceforge.net/projects/archcraft/files/${RELEASE}/download"
    echo "${URL} ${HASH}"
}

