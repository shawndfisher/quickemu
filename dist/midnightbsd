#!/usr/bin/env bash

PRETTY_NAME="MidnightBSD"
HOMEPAGE="https://www.midnightbsd.org/"

GUEST="midnightbsd"

function dist_releases() {
    echo 3.1.4
}

function dist_image() {
    local HASH=""
    local URL="http://ns3.foolishgames.net/ftp/pub/MidnightBSD/releases/amd64/ISO-IMAGES/${RELEASE}"
    local ISO="MidnightBSD-${RELEASE}--amd64-disc1.iso"
    echo "${URL}/${ISO} ${HASH}"
}

