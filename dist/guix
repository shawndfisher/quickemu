function dist_releases() {
    echo 1.4.0 1.3.0
}

function dist_image() {
    local HASH=""
    local ISO="guix-system-install-${RELEASE}.x86_64-linux.iso"
    local URL="https://ftpmirror.gnu.org/gnu/guix"
    echo "${URL}/${ISO} ${HASH}"
}

function dist_image_handle() {
    echo
    #cp ${VM_PATH}/${ISO} ${VM_PATH}/disc.qcow2
}

