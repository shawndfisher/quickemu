#!/usr/bin/env bash

INFO="LXLE|-|https://lxle.net/|Security-oriented, lightweight Linux distribution based on musl libc and busybox."

function dist_releases() {
    echo focal
}

function dist_image_get() {
    local HASH=""
    local URL=""
    local ISO=""
    URL="https://sourceforge.net/projects/lxle/files/Final/OS/${RELEASE^}-64"
    ISO="LXLE-${RELEASE^}-Release.iso"
    HASH="$(wget -q -O- "${URL}/${ISO}.md5" | cut -d' ' -f1)"
    echo "${URL}/${ISO} ${HASH}"
}

# vim:tabstop=4:shiftwidth=4:expandtab
