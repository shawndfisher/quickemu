#!/usr/bin/env bash

INFO="netboot.xyz|-|https://netboot.xyz/|Your favorite operating systems in one place."

function dist_releases() {
    echo latest
}

function dist_image_get() {
    local HASH=""
    local ISO="netboot.xyz.iso"
    local URL="https://boot.netboot.xyz/ipxe"
    HASH=$(web_pipe "${URL}/netboot.xyz-sha256-checksums.txt" | grep "${ISO}" | cut -d' ' -f1)
    echo "${URL}/${ISO} ${HASH}"
}

# vim:tabstop=4:shiftwidth=4:expandtab
